	# 位置 nginx.conf -> http
    # include 'snippets/gzip'

    gzip on;
    gzip_min_length 1k;
    gzip_comp_level 9;
    gzip_types text/xml text/plain application/json text/css image/svg application/font-woff application/vnd.ms-fontobject application/vnd.apple.mpegurl application/javascript image/x-icon image/jpeg image/gif image/png;
    gzip_vary on;
    gzip_disable "MSIE [1-6]\.";
    gzip_buffers 32 4k;
    gzip_http_version 1.0;
