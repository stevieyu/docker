[mysqladmin]
user=root

[mysqld]
basedir=/opt/lamp/mysql
datadir=/opt/lamp/mysql/data
port=3306
socket=/opt/lamp/mysql/tmp/mysql.sock
tmpdir=/opt/lamp/mysql/tmp
max_allowed_packet=16M
#bind-address=127.0.0.1

character-set-server=UTF8
collation-server=utf8_general_ci
[mysqld_safe]
mysqld=mysqld.bin

[client]
port=3306
socket=/opt/lamp/mysql/tmp/mysql.sock

default-character-set=UTF8
[manager]
port=3306
socket=/opt/lamp/mysql/tmp/mysql.sock
pid-file=/opt/lamp/mysql/tmp/manager.pid
default-mysqld-path=/opt/lamp/mysql/bin/mysqld.bin